    typename T::element_type;
    { *ptr } -&gt; std::convertible_to&lt;typename T::element_type&amp;&gt;;
    { ptr.get() } -&gt; std::convertible_to&lt;typename T::element_type*&gt;;
} &amp;&amp; (std::is_same_v&lt;T, std::shared_ptr&lt;typename T::element_type&gt;&gt; ||
